CREATE OR REPLACE PROCEDURE DEMO_SHOP.remove_room_by_name (NAME_IN IN rooms.NAME%TYPE)

IS

 

BEGIN

   IF NAME_IN IS NULL

   THEN

      RAISE_APPLICATION_ERROR(-20105,'My exception');

   END IF;

 

   DELETE FROM ROOMS

            WHERE NAME LIKE NAME_IN;

 

END REMOVE_ROOM_BY_NAME;
/


CREATE OR REPLACE PUBLIC SYNONYM REMOVE_ROOM_BY_NAME FOR DEMO_SHOP.REMOVE_ROOM_BY_NAME;


GRANT EXECUTE, DEBUG ON DEMO_SHOP.REMOVE_ROOM_BY_NAME TO PUBLIC;
